-----------open_session-----------

PAM_TTY=ssh
PAM_USER=nian
PWD=/
XDG_SESSION_TYPE=tty
PAM_RHOST=127.0.0.1
SSH_CONNECTION=127.0.0.1 58540 127.0.0.1 22
PAM_SERVICE=sshd
XDG_SESSION_CLASS=user
SHLVL=1
XDG_SESSION_ID=7
XDG_RUNTIME_DIR=/run/user/1000
DBUS_SESSION_BUS_ADDRESS=unix:path=/run/user/1000/bus
SSH_AUTH_INFO_0=password

MAIL=/var/spool/mail/nian
PAM_TYPE=open_session
_=/usr/bin/env

-----------close_session-----------

PAM_TTY=ssh
PAM_USER=nian
PWD=/
XDG_SESSION_TYPE=tty
PAM_RHOST=127.0.0.1
SSH_CONNECTION=127.0.0.1 58540 127.0.0.1 22
PAM_SERVICE=sshd
XDG_SESSION_CLASS=user
SHLVL=1
XDG_SESSION_ID=7
XDG_RUNTIME_DIR=/run/user/1000
DBUS_SESSION_BUS_ADDRESS=unix:path=/run/user/1000/bus
SSH_AUTH_INFO_0=password

PAM_TYPE=close_session
_=/usr/bin/env
